<style>
    * {
        margin: 0;
        padding: 0;
        font-family: Verdana, sans-serif;
        vertical-align: middle;
        -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
                user-select: none;
        overflow: hidden;
        background-color: rgb(215, 215, 215);
        color: rgb(70, 70, 70);
    }
    img {
        width: 100%;
        height: 100%;
    }
</style>
<main>
    <img src="/izobr/poverni.jpg" id="imger">
    <h2 id="choto" hidden="true"><%=sila%></h2>
</main>
<script>
    const ifer = setInterval(() => {
        if (document.documentElement.clientWidth > document.documentElement.clientHeight) {
            clearInterval(ifer);
            window.location.href = document.getElementById('choto').innerText;
        }
    }, 100);
</script>